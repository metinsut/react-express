{
      "name": "node-react",
      "version": "0.1.0",
      "private": true,
      "dependencies": {
            "axios": "^0.18.0",
            "bcrypt": "^1.0.3",
            "body-parser": "^1.18.2",
            "express": "^4.16.3",
            "jsonwebtoken": "^8.2.0",
            "mongoose": "^5.0.12",
            "node-sass-chokidar": "^1.2.2",
            "nodemon": "^1.17.3",
            "npm-run-all": "^4.1.2",
            "react": "^16.3.1",
            "react-dom": "^16.3.1",
            "react-redux": "^5.0.7",
            "react-router-dom": "^4.2.2",
            "react-scripts": "1.1.4",
            "redux": "^3.7.2",
            "redux-form": "^7.3.0",
            "redux-logger": "^3.0.6",
            "redux-thunk": "^2.2.0"
      },
      "scripts": {
            "node": "nodemon server.js",
            "eject": "react-scripts eject",
            "build-css": "node-sass-chokidar src/static/css -o src/static/css/",
            "watch-css": "npm run build-css && node-sass-chokidar src/static/css/ -o src/static/css/ --watch --recursive",
            "start-js": "react-scripts start",
            "start": "npm-run-all -p watch-css start-js",
            "build-js": "react-scripts build",
            "build": "npm-run-all build-css build-js"
      }
}
